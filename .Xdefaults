URxvt.font: xft:Meslo\ LG\ S\ DZ\ for\ Powerline:style=RegularForPowerline:size=10, \
            xft:Pomodoro:size=9, \
            xft:FontAwesome:size=9, \
            xft:icomoon:size=9, \
            xft:octicons:size=6
#Xft.autohint: 0
#Xft.lcdfilter: lcddefault
#Xft.hintstyle: hintslight
#Xft.hinting: 1
#Xft.antialias: 1
